{
  "name": "electron-babel-react",
  "version": "1.0.0",
  "description": "minimal react+electron+webpack starter",
  "author": "Tristan Teufel",
  "homepage": "teufel-it.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/firsttris/electron-babel-react.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"yarn dev-server\" \"wait-on http://localhost:8080 && yarn electron-dev\"",
    "test": "jest --coverage",
    "test-watch": "jest --coverage --watchAll",
    "electron-prod": "electron .",
    "electron-dev": "electron . --dev",
    "dev-server": "webpack-dev-server --hot --config=./webpack.dev.config.js",
    "webpack-build": "webpack --config webpack.prod.config.js",
    "electron-package": "electron-packager ./ --out=./builds --overwrite",
    "package": "yarn webpack-build && yarn electron-package"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.1",
    "bootstrap": "4.0.0-alpha.6",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.4",
    "electron": "^1.6.11",
    "electron-packager": "^8.7.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.6.1",
    "react-transition-group": "^1.2.0",
    "reactstrap": "^4.6.2",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.18.2",
    "wait-on": "^2.0.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "jest": {
    "transform": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./jest/fileTransformer.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
